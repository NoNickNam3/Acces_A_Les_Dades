<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_2.xsd" version="2.2">
    <embeddable class="org.milaifontanals.biblioteca.Telefon" access="FIELD" metadata-complete="true">
        <attributes>
            <basic name="numero" optional="false">
                <column nullable="false" length="15"/>
            </basic>
            <basic name="tipus" optional="false">
                <column column-definition="char check (tipus in ('M','F'))"/>
            </basic>
        </attributes>
    </embeddable>
</entity-mappings>
